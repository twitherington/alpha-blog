<h1>Articles</h1>
<%= link_to "Create new article", new_article_path %>
<table>
  <tr>
    <th>
      Title
    </th>
    <th>
      Description
    </th>
  </tr>

  <% @articles.each do |a| %>
  <tr>
    <td>
      <%= a.title %>
    </td>
    <td>
      <%= a.description %>
    </td>
    <td><%= link_to 'Edit', edit_article_path(a) %></td>
    <td><%= link_to 'Show', article_path(a) %></td>
  </tr>
  <% end %>

</table>
